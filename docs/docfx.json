{
  "metadata": [
    {
      "src": [
        {
          "files": [
			"**/Optimizely.cs",
			"**/NotificationCenter.cs",
			"**/IEventDispatcher.cs",
			"**/ILogger.cs",
			"**/IErrorHandler.cs",
			"**/UserProfileService.cs",
			"**/UserAttributes.cs",
			"**/EventAttributes.cs",
			"**/FeatureVariable.cs"
		  ],
		  "src": "../OptimizelySDK"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "csharp/**"
        ]
      }
    ],
    "dest": "csharp",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}